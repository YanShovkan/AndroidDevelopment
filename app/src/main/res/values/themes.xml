<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ShovkanLab1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/green_200</item>
        <item name="colorPrimaryVariant">@color/green_700</item>
        <item name="colorOnPrimary">@color/black_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_200</item>
        <item name="colorSecondaryVariant">@color/green_200</item>
        <item name="colorOnSecondary">@color/black_200</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.ShovkanLab1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ShovkanLab1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ShovkanLab1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.ShovkanLab1.AppWidgetContainerParent" parent="@android:style/Theme.DeviceDefault">
        <item name="appWidgetRadius">8dp</item>
        <item name="appWidgetPadding">4dp</item>
        <item name="appWidgetInnerRadius">4dp</item>
    </style>

    <style name="Theme.ShovkanLab1.AppWidgetContainer" parent="Theme.ShovkanLab1.AppWidgetContainerParent" />
</resources>